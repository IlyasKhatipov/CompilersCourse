class Animal is
  method speak() : Void =>
    output("...")
end

class Dog is
  method speak() : Void =>
    output("woof")
end

class Main is
  var a : Animal;
  var d : Dog;
  method main() : Void =>
    a := d;       # полиморфизм
    a.speak()     # должен вызвать Dog.speak()
end